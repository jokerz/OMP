				<form action="/app.mpl" name="registMedia" id="registMedia" method="POST">
				<table border="1" cellspacing="0" cellpadding="2" width="100%" rules="all" class="datagrid">
					<tr><th align="center" colspan="3"> 広告新規登録フォーム</th></tr>
